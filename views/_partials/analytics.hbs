{{#has_analytics}}
<!-- Google Universal Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id={{ get_analytics }}"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', '{{ get_analytics }}');
</script>
<!-- End Google Universal Analytics -->
{{/has_analytics}}
